{
  "context7": {
    "command": "cmd",
    "args": ["/c", "npx", "-y", "@upstash/context7-mcp@latest", "--api-key", "${CONTEXT7_API_KEY}"]
  },

  "brave-search": {
    "command": "cmd",
    "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-brave-search@latest"],
    "env": {
      "BRAVE_API_KEY": "${BRAVE_API_KEY}"
    }
  },

  "playwright": {
    "command": "cmd",
    "args": ["/c", "npx", "-y", "@playwright/mcp@latest"]
  },

  "github": {
    "command": "cmd",
    "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-github@latest"],
    "env": {
      "GITHUB_TOKEN": "${GITHUB_TOKEN}"
    }
  }
}
